<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="200.0" maxWidth="300.0" minHeight="160.0" minWidth="300.0" prefHeight="160.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <CheckBox id="short_check" fx:id="short_check" layoutX="13.0" layoutY="14.0" mnemonicParsing="false" style="-fx-cursor: hand;" text="Короткий день">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </CheckBox>
      <Label id="main_label" fx:id="main_label" alignment="CENTER" contentDisplay="CENTER" layoutY="72.0" prefHeight="21.0" prefWidth="300.0" text="До конца рабочего дня осталось:" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Label id="time_count" fx:id="time_count" alignment="CENTER" layoutX="1.0" layoutY="115.0" prefHeight="21.0" prefWidth="300.0" text="Label">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <CheckBox id="up_check" fx:id="up_check" layoutX="13.0" layoutY="36.0" mnemonicParsing="false" style="-fx-cursor: hand;" text="Поверх всех окон">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
